<h1>登陆</h1>
	<%= form_for(:session, url: sessions_path) do |f| %>	
		<td>电子邮箱</td>
		<td><%= f.text_field :email %> </td>
		<td>密码</td>
		<td><%= f.password_field :password %> </td>  
		<td><%= f.submit "登陆", class: "btn btn-large btn-primary" %></td>			
	<% end %>	
<p> 新用户? <%= link_to "马上注册!", signup_path %> </p>